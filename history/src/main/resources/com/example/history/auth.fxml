<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="589.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.history.AuthController">
   <children>
      <ImageView fitHeight="353.0" fitWidth="602.0" layoutY="47.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@stars3.png" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="77.0" prefWidth="588.0" style="-fx-background-color: #8b00ff;">
         <children>
            <Button fx:id="StartButton" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#OnStartButtonClick" prefHeight="26.0" prefWidth="223.0" style="-fx-background-color: #ff0000; -fx-border-radius: 5;" text="Вернуться на начальную страницу" />
            <Button fx:id="SignUpButton" layoutX="397.0" layoutY="26.0" mnemonicParsing="false" onAction="#OnSignUpButtonClick" prefHeight="26.0" prefWidth="145.0" style="-fx-background-color: #ff0000; -fx-background-radius: 5;" text="Зарегистрироваться" />
         </children></AnchorPane>
      <Button fx:id="SignInButton" layoutX="277.0" layoutY="340.0" mnemonicParsing="false" onAction="#OnSignInButtonClick" text="Войти" />
      <PasswordField fx:id="password_field" layoutX="175.0" layoutY="285.0" prefHeight="26.0" prefWidth="252.0" promptText="Пароль" />
      <TextField fx:id="login_field" layoutX="175.0" layoutY="230.0" prefHeight="26.0" prefWidth="252.0" promptText="Логин" />
      <TextArea fx:id="err" layoutX="171.0" layoutY="95.0" prefHeight="104.0" prefWidth="252.0" />
   </children>
</AnchorPane>
